<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.homebusinessfinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.Login"
            android:label="@string/welcome"
            android:parentActivityName=".Activities.MainActivity" />
        <activity
            android:name=".Activities.CreateAccount"
            android:label="@string/welcome"
            android:parentActivityName=".Activities.MainActivity" />
        <activity
            android:name=".Activities.Profile"
            android:label="@string/profile"
            android:parentActivityName=".Activities.MainActivity" />
        <activity android:name=".Activities.AddBusiness"
            android:label="@string/addbusiness"
            android:parentActivityName=".Activities.MainActivity"></activity>
    </application>

</manifest>